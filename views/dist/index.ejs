        <style>
            .content-img {
                animation: dashed-circle 10s ease infinite alternate;
                }
                @keyframes dashed-circle{
                from{
                    transform: rotate(0deg);
                }
                to{
                    transform: rotate(360deg);
                }
            }
            .animate-motion {
                animation: motion 7s infinite !important;
                }
                @keyframes motion{
                0% {
                    transform: translateY(0);
                }
                25% {
                    transform: translate(-15px, -15px);
                }
                50% {
                    transform: translate(-8px, -8px);

                }
                75% {
                    transform: translate(-15px, 8px);
                }
                100% {
                    transform: translateY(0);
                }
            }
            .swigo-logo {
                animation: swigo-logo 2s infinite !important;
                }
                @keyframes swigo-logo {
                0% {
                    transform: rotate(0);
                }
                45% {
                    transform: translate3d(-4%, -13%, 0) rotate(-13deg);
                }
                55% {
                    transform: translate3d(1%, -2%, 0) rotate(6deg);
                }
                65% {
                    transform: translate3d(-2%, -4%, 0) rotate(-3deg);
                }
                75% {
                    transform: translate3d(2%, 3%, 0) rotate(3deg);
                }
                80% {
                    transform: translate3d(-2%, 1%, 0) rotate(-2deg);
                }
                100% {
                    transform: none;
                }
            }
            .animate-move {
                animation: move 7s infinite !important;
            }   
            @keyframes move {
                0% {
                transform: translateY(0);
            }
            25% {
                transform: translateY(-25px);
            }
            50% {
                transform: translateY(-15px);
            }
            75% {
                transform: translateY(-25px);
            }
            100% {
                transform: translateY(0);
            }
        }
        .cursor {
            width: 50px;
            height: 50px;
            border-radius: 100%;
            border: 1px solid green;
            transition: all 200ms ease-out;
            position: fixed;
            pointer-events: none;
            left: 0;
            top: 0;
            transform: translate(calc(-50% + 15px), -50%);
            }

            .cursor2 {
            width: 20px;
            height: 20px;
            border-radius: 100%;
            background-color: yellow;
            opacity: .3;
            position: fixed;
            transform: translate(-50%, -50%);
            pointer-events: none;
            transition: width .3s, height .3s, opacity .3s;
            }

            /* back to top */
            .back-to-top{
                display: none;
            }
            .top{
                display: flex;
                position: fixed;
                transition: all 0.4s;
                bottom: 5%;
                right: 3%;
                width: 20px;
                height: 20px;
                border-radius: 50%;
                padding: 20px;
                background-color: transparent;
                border: 1px solid #7CA549;
                justify-content: center;
                align-items: center;
                z-index: 999;
            }
            .top a{
                color: #7CA549;
            }
            .dropdown ul li a:hover{
                color: #7CA549;
            }
            .hidden{
                overflow: hidden;
            }
            .loader{
                width: 100%;
                height: 100vh;
                background-color: white;
                position: fixed;
                z-index: 99999;
                display: flex;
                justify-content: center;
                align-items: center;
            }
            .today-img::after{
                    content: "";
                    position: absolute;
                    opacity: 0;
                    top: 0%;
                    left: 0;
                    width: 100%;
                    height: 100%;
                    transition: all 0.5s;
                    z-index: 1;
                    background-image: linear-gradient(to top, #222 0.33%, rgba(34, 34, 34, 0) 101%);
            }
            .img-hov{
                content: "";
                position: absolute;
                top: -100%;
                background-color: var(--primary);
                z-index: 0;
                width: 335px;
                height: 335px;
                border-radius: 50%;
                transform: translateX(-50%);
                left: 50%;
                transition: all 0.5s;
            }
        </style>
