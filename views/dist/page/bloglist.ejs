        <script>
            //fixed-header
            $(window).scroll(function () {
                if ($(window).scrollTop() > 300) {
                    $(".head").addClass("fixed-header");
                    $(".header-icon").addClass("color");
                    $("nav").addClass("text-blacck");
                    $(".logo-dark").removeClass("hidden");
                    $(".logo-white").addClass("hidden");
                }
                else {
                    $(".head").removeClass("fixed-header");
                    $(".header-icon").removeClass("color");
                    $("nav").removeClass("text-blacck");
                    $(".logo-dark").addClass("hidden");
                    $(".logo-white").removeClass("hidden");
                }
            });
        </script> 
