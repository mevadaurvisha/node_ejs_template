        <script>
            $(document).ready(function(){
                $(".sidebar-dropdown > a").click(function () {
                $(".sidebar-submenu").slideUp(200);
                    if ($(this).parent().hasClass("active")) {
                        $(".sidebar-dropdown").removeClass("active");
                        $(this).parent().removeClass("active");
                    } else {
                        $(".sidebar-dropdown").removeClass("active");
                        $(this).next(".sidebar-submenu").slideDown(200);
                        $(this).parent().addClass("active");
                    }
                });
            });
            
        </script>
        <script>
            //fixed-header
            $(window).scroll(function () {
                if ($(window).scrollTop() > 300) {
                    $(".head").addClass("fixed-header");
                    $(".header-icon").addClass("color");
                    $("nav").addClass("text-blacck");
                    $(".logo-dark").removeClass("hidden");
                    $(".logo-white").addClass("hidden");
                }
                else {
                    $(".head").removeClass("fixed-header");
                    $(".header-icon").removeClass("color");
                    $("nav").removeClass("text-blacck");
                    $(".logo-dark").addClass("hidden");
                    $(".logo-white").removeClass("hidden");
                }
            });
        </script> 
        <script>
    
            $(window).scroll(function(){
                
                if ($(this).scrollTop() > 400){
            
                    $('.back-to-top').addClass("top");
            
                }else{
                    
                    $('.back-to-top').removeClass("top");
            
                }
            })
        </script>
